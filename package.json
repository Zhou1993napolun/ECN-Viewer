{
  "name": "ecn-viewer",
  "version": "1.0.0",
  "description": "Edgeworx ioFog Edge Cloud Network Viewer project  \\nCopyright (c) 2019 Edgeworx, Inc.",
  "main": "./server/index.js",
  "author": "Alexandre de Wergifosse",
  "contributors": [
    "Serge Radinovic"
  ],
  "license": "MIT",
  "homepage": "https://www.edgeworx.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/Edgeworx/ECN-viewer"
  },
  "dependencies": {
    "@material-ui/core": "4.0.0-rc.0",
    "@material-ui/icons": "4.2.0",
    "@material-ui/styles": "4.1.1",
    "downshift": "3.2.10",
    "google-map-react": "1.1.4",
    "lodash": "4.17.14",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-json-view": "1.19.1",
    "react-loading-skeleton": "1.1.2",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1"
  },
  "proxy": "http://localhost:5555",
  "scripts": {
    "start": "npm run build && npm run prod",
    "start-dev": "cp controller_dev.json ./src/ControllerProvider/controller.json && concurrently 'PORT=5555 nodemon server/index.js' 'react-scripts start'",
    "prod": "node server/index.js",
    "build": "cp controller_default.json ./src/ControllerProvider/controller.json && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "cd server && npm i"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30000984"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "react-scripts": "3.0.1"
  }
}
